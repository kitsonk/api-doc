{
	"compilerOptions": {
		"module": "umd",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"inlineSourceMap": true,
		"inlineSources": true,
		"lib": [
			"dom",
			"es5",
			"es2015.iterable",
			"es2015.symbol",
			"es2015.symbol.wellknown"
		],
		"outDir": "_build",
		"strictNullChecks": true,
		"target": "es5"
	},
	"include": [
		"./src/**/*.ts",
		"./tests/**/*.ts"
	]
}